amends "../../pkg/schemas/Config.pkl"
import "../../pkg/schemas/aws/EC2.pkl"
import "../../pkg/schemas/aws/S3.pkl"

aws {
  region = "us-east-1"

  buckets = new Listing {
    new S3.Bucket {
      bucket = "my-test-bucket-12345"
      acl = "private"
    }
  }

  instances = new Listing {
    new EC2.Instance {
      ami = "ami-0c55b159cbfafe1f0"
      instanceType = "t2.micro"
      tags = new Mapping {
        ["Name"] = "TestInstance"
      }
    }
  }
}
