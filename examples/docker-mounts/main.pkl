amends "../../pkg/schemas/Config.pkl"
import "../../pkg/schemas/docker/Docker.pkl"

docker {
  volumes {
    ["picklr-data"] {}
  }

  containers {
    ["picklr-db"] {
      image = "alpine:latest"
      command { "tail"; "-f"; "/dev/null" }
      volumes {
        "picklr-data:/data"
      }
    }
  }
}
