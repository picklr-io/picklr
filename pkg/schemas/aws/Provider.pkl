module picklr.aws.Provider

import "../Resource.pkl"
import "ACM.pkl"
import "APIGateway.pkl"
import "AutoScaling.pkl"
import "CloudFront.pkl"
import "CloudWatch.pkl"
import "DynamoDB.pkl"
import "EC2.pkl"
import "ECR.pkl"
import "ECS.pkl"
import "ELBv2.pkl"
import "EventBridge.pkl"
import "IAM.pkl"
import "KMS.pkl"
import "Lambda.pkl"
import "RDS.pkl"
import "Route53.pkl"
import "S3.pkl"
import "SecretsManager.pkl"
import "SNS.pkl"
import "SQS.pkl"

/// Configuration for the AWS Provider.
class Config {
  /// The region where AWS operations will take place. Examples are us-east-1, us-west-2, etc.
  region: String

  /// The profile for API operations. If not set, the default profile created with `aws configure` will be used.
  profile: String?

  /// Shared credentials file path.
  sharedCredentialsFile: String?

  /// Access key for API operations.
  accessKey: String?

  /// Secret key for API operations.
  secretKey: String?

  /// List of S3 Buckets to manage.
  buckets: Listing<S3.Bucket>?

  /// List of EC2 Instances to manage.
  instances: Listing<EC2.Instance>?

  /// List of Launch Templates.
  launchTemplates: Listing<EC2.LaunchTemplate>?

  /// List of Key Pairs.
  keyPairs: Listing<EC2.KeyPair>?

  /// List of Auto Scaling Groups.
  autoScalingGroups: Listing<AutoScaling.AutoScalingGroup>?

  /// List of VPCs.
  vpcs: Listing<EC2.Vpc>?

  /// List of Subnets.
  subnets: Listing<EC2.Subnet>?

  /// List of Security Groups.
  securityGroups: Listing<EC2.SecurityGroup>?

  /// List of Internet Gateways.
  internetGateways: Listing<EC2.InternetGateway>?

  /// List of Elastic IPs.
  elasticIPs: Listing<EC2.ElasticIP>?

  /// List of NAT Gateways.
  natGateways: Listing<EC2.NatGateway>?

  /// List of Route Tables.
  routeTables: Listing<EC2.RouteTable>?

  /// List of IAM Roles.
  roles: Listing<IAM.Role>?

  /// List of IAM Policies.
  policies: Listing<IAM.Policy>?

  /// List of IAM Instance Profiles.
  instanceProfiles: Listing<IAM.InstanceProfile>?

  /// List of S3 Bucket Policies.
  bucketPolicies: Listing<S3.BucketPolicy>?

  /// List of Lambda Functions.
  functions: Listing<Lambda.Function>?

  /// List of DynamoDB Tables.
  tables: Listing<DynamoDB.Table>?

  /// List of EDS Volumes.
  volumes: Listing<EC2.Volume>?

  /// List of Network ACLs.
  networkAcls: Listing<EC2.NetworkAcl>?

  /// List of VPC Peering Connections.
  vpcPeeringConnections: Listing<EC2.VpcPeeringConnection>?

  /// List of Transit Gateways.
  transitGateways: Listing<EC2.TransitGateway>?

  /// List of Transit Gateway Attachments.
  transitGatewayAttachments: Listing<EC2.TransitGatewayAttachment>?

  /// List of VPC Endpoints.
  vpcEndpoints: Listing<EC2.VpcEndpoint>?


  /// List of RDS Instances.
  dbInstances: Listing<RDS.Instance>?

  /// List of RDS DB Subnet Groups.
  dbSubnetGroups: Listing<RDS.DBSubnetGroup>?

  /// List of RDS DB Clusters.
  dbClusters: Listing<RDS.DBCluster>?

  /// List of SQS Queues.
  queues: Listing<SQS.Queue>?

  /// List of SNS Topics.
  topics: Listing<SNS.Topic>?

  /// List of SNS Subscriptions.
  subscriptions: Listing<SNS.Subscription>?

  /// List of ECR Repositories.
  repositories: Listing<ECR.Repository>?

  /// List of ECS Clusters.
  clusters: Listing<ECS.Cluster>?

  /// List of ECS Task Definitions.
  taskDefinitions: Listing<ECS.TaskDefinition>?

  /// List of ECS Services.
  services: Listing<ECS.Service>?

  /// List of ELBv2 Load Balancers.
  loadBalancers: Listing<ELBv2.LoadBalancer>?

  /// List of ELBv2 Target Groups.
  targetGroups: Listing<ELBv2.TargetGroup>?

  /// List of ELBv2 Listeners.
  listeners: Listing<ELBv2.Listener>?

  /// List of Route53 Hosted Zones.
  hostedZones: Listing<Route53.HostedZone>?

  /// List of Route53 Record Sets.
  recordSets: Listing<Route53.RecordSet>?

  /// List of API Gateway REST APIs.
  restApis: Listing<APIGateway.RestApi>?

  /// List of API Gateway Resources.
  apiResources: Listing<APIGateway.ApiResource>?

  /// List of API Gateway Methods.
  methods: Listing<APIGateway.Method>?

  /// List of API Gateway Deployments.
  deployments: Listing<APIGateway.Deployment>?

  /// List of CloudFront Distributions.
  distributions: Listing<CloudFront.Distribution>?

  /// List of CloudWatch Log Groups.
  logGroups: Listing<CloudWatch.LogGroup>?

  /// List of CloudWatch Alarms.
  alarms: Listing<CloudWatch.Alarm>?

  /// List of KMS Keys.
  keys: Listing<KMS.Key>?

  /// List of KMS Aliases.
  aliases: Listing<KMS.Alias>?

  /// List of Secrets Manager Secrets.
  secrets: Listing<SecretsManager.Secret>?

  /// List of Secrets Manager Secret Versions.
  secretVersions: Listing<SecretsManager.SecretVersion>?

  /// List of ACM Certificates.
  certificates: Listing<ACM.Certificate>?

  /// List of ACM Certificate Validations.
  certificateValidations: Listing<ACM.CertificateValidation>?

  /// List of EventBridge Event Buses.
  eventBuses: Listing<EventBridge.EventBus>?

  /// List of EventBridge Rules.
  rules: Listing<EventBridge.Rule>?

  /// List of EventBridge Targets.
  targets: Listing<EventBridge.Target>?

  /// Flattened list of all AWS resources.
  allResources: Listing<Resource.Resource> = new Listing {
    ...(if (buckets != null) buckets else new Listing {})
    ...(if (instances != null) instances else new Listing {})
    ...(if (launchTemplates != null) launchTemplates else new Listing {})
    ...(if (keyPairs != null) keyPairs else new Listing {})
    ...(if (autoScalingGroups != null) autoScalingGroups else new Listing {})
    ...(if (networkAcls != null) networkAcls else new Listing {})
    ...(if (vpcPeeringConnections != null) vpcPeeringConnections else new Listing {})
    ...(if (transitGateways != null) transitGateways else new Listing {})
    ...(if (transitGatewayAttachments != null) transitGatewayAttachments else new Listing {})
    ...(if (vpcEndpoints != null) vpcEndpoints else new Listing {})
    ...(if (vpcs != null) vpcs else new Listing {})
    ...(if (subnets != null) subnets else new Listing {})
    ...(if (securityGroups != null) securityGroups else new Listing {})
    ...(if (internetGateways != null) internetGateways else new Listing {})
    ...(if (elasticIPs != null) elasticIPs else new Listing {})
    ...(if (natGateways != null) natGateways else new Listing {})
    ...(if (routeTables != null) routeTables else new Listing {})
    ...(if (roles != null) roles else new Listing {})
    ...(if (policies != null) policies else new Listing {})
    ...(if (instanceProfiles != null) instanceProfiles else new Listing {})
    ...(if (bucketPolicies != null) bucketPolicies else new Listing {})
    ...(if (functions != null) functions else new Listing {})

    ...(if (tables != null) tables else new Listing {})
    ...(if (volumes != null) volumes else new Listing {})
    ...(if (dbInstances != null) dbInstances else new Listing {})
    ...(if (dbSubnetGroups != null) dbSubnetGroups else new Listing {})
    ...(if (dbClusters != null) dbClusters else new Listing {})
    ...(if (queues != null) queues else new Listing {})

    ...(if (topics != null) topics else new Listing {})
    ...(if (subscriptions != null) subscriptions else new Listing {})
    ...(if (repositories != null) repositories else new Listing {})
    ...(if (clusters != null) clusters else new Listing {})
    ...(if (taskDefinitions != null) taskDefinitions else new Listing {})
    ...(if (services != null) services else new Listing {})
    ...(if (loadBalancers != null) loadBalancers else new Listing {})
    ...(if (targetGroups != null) targetGroups else new Listing {})
    ...(if (listeners != null) listeners else new Listing {})
    ...(if (hostedZones != null) hostedZones else new Listing {})
    ...(if (recordSets != null) recordSets else new Listing {})
    ...(if (restApis != null) restApis else new Listing {})
    ...(if (apiResources != null) apiResources else new Listing {})
    ...(if (methods != null) methods else new Listing {})
    ...(if (deployments != null) deployments else new Listing {})
    ...(if (distributions != null) distributions else new Listing {})
    ...(if (logGroups != null) logGroups else new Listing {})
    ...(if (alarms != null) alarms else new Listing {})
    ...(if (keys != null) keys else new Listing {})
    ...(if (aliases != null) aliases else new Listing {})
    ...(if (secrets != null) secrets else new Listing {})
    ...(if (secretVersions != null) secretVersions else new Listing {})
    ...(if (certificates != null) certificates else new Listing {})
    ...(if (certificateValidations != null) certificateValidations else new Listing {})
    ...(if (eventBuses != null) eventBuses else new Listing {})
    ...(if (rules != null) rules else new Listing {})
    ...(if (targets != null) targets else new Listing {})
  }
}
