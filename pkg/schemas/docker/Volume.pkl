module picklr.docker.Volume

import "../Resource.pkl" as Core

class Volume extends Core.Resource {
  provider = "docker"
  type = "docker_volume"

  hidden name: String = ""

  /// The driver to use (e.g., "local").
  hidden driver: String = "local"

  properties = new {
    ["name"] = name
    ["driver"] = driver
  }
}
